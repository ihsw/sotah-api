{
  "name": "sotah-api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "test": "ava -vs ./dist/test/*.js ./dist/test/**/*.js",
    "test:w": "ava -vsw ./dist/test/*.js ./dist/test/**/*.js",
    "test-coverage": "nyc --reporter=lcov npm test",
    "test-coverage-report": "nyc report",
    "test-coverage-check": "nyc check-coverage --lines 95 --functions 75 --branches 95",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "tsc && echo built",
    "build:w": "tsc -w && echo built",
    "ava": "ava --verbose",
    "lint": "tslint ./src/**/*.ts && echo linted",
    "migrate-up": "sequelize db:migrate --config ./dist/sequelize-config.js --migrations-path ./dist/migrations",
    "migrate-down": "sequelize db:migrate:undo --config ./dist/sequelize-config.js --migrations-path ./dist/migrations"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihsw/sotah-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ihsw/sotah-api/issues"
  },
  "homepage": "https://github.com/ihsw/sotah-api#readme",
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.11.1",
    "@types/http-status": "^0.2.29",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/passport": "^0.4.4",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.13",
    "@types/supertest": "^2.0.2",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.5",
    "ava": "^0.21.0",
    "coveralls": "^2.13.1",
    "nyc": "^11.1.0",
    "supertest": "^3.0.0",
    "tslint": "^5.6.0",
    "typescript": "^2.8.1",
    "uuid": "^3.2.1"
  },
  "dependencies": {
    "async-middleware": "^1.2.1",
    "bcrypt": "^1.0.3",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.2.1",
    "nats": "^0.7.20",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.1",
    "sequelize": "^4.37.5",
    "sequelize-cli": "^4.0.0",
    "winston": "^2.3.1"
  },
  "nyc": {
    "exclude": []
  }
}
