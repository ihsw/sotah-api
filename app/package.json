  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "test": "ava -vs './dist/test/**/*.js'",
    "test:w": "ava -vsw './dist/test/**/*.js'",
    "test-coverage": "nyc --reporter=lcov npm test",
    "test-coverage-report": "nyc report",
    "test-coverage-check": "nyc check-coverage --lines 95 --functions 75 --branches 95",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "tsc && echo built",
    "build:w": "tsc -w && echo built",
    "ava": "ava --verbose",
    "lint": "tslint './src/**/*.ts' && echo linted",
    "migrate-up": "typeorm migration:run",
    "migrate-down": "typeorm migration:revert",
    "run:w": "nodemon --watch ./dist ./dist/index.js",
    "run": "node ./dist/index.js",
    "run:prod": "node ./dist/index-prod.js"
  },
  "repository": {
    "type": "git",
